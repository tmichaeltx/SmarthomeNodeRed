["bfbb9354.3ada6"]]},{"id":"c1e21f98.3f35a","type":"hb-event","z":"42fe707d.eeeb6","name":"Evas Bedroom Smoke","Homebridge":"Homebridge","Manufacturer":"Nest","Service":"Smoke Sensor","device":"HomebridgeCC:22:3D:E3:CE:30NestEvas Bedroom Smoke00000087","conf":"df59aa47.b26148","x":720,"y":3360,"wires":[["6381125f.91754c"]]},{"id":"32f1e87d.57f0b8","type":"hb-event","z":"42fe707d.eeeb6","name":"Game Room Smoke","Homebridge":"Homebridge","Manufacturer":"Nest","Service":"Smoke Sensor","device":"HomebridgeCC:22:3D:E3:CE:30NestGame Room Smoke00000087","conf":"df59aa47.b26148","x":710,"y":3440,"wires":[["1f37b7ad.e1ac38"]]},{"id":"ed07edd8.39eab","type":"hb-event","z":"42fe707d.eeeb6","name":"Guest Bedroom Smoke","Homebridge":"Homebridge","Manufacturer":"Nest","Service":"Smoke Sensor","device":"HomebridgeCC:22:3D:E3:CE:30NestGuest Bedroom Smoke00000087","conf":"df59aa47.b26148","x":720,"y":3520,"wires":[["7c12c960.ed5bc8"]]},{"id":"c3a7eb8a.1170a8","type":"hb-event","z":"42fe707d.eeeb6","name":"Master Bedroom Smoke","Homebridge":"Homebridge","Manufacturer":"Nest","Service":"Smoke Sensor","device":"HomebridgeCC:22:3D:E3:CE:30NestMaster Bedroom Smoke00000087","conf":"df59aa47.b26148","x":730,"y":3600,"wires":[["3da94c95.1e3514"]]},{"id":"5a4d8d36.5e2b54","type":"hb-event","z":"42fe707d.eeeb6","name":"Toy Room Smoke","Homebridge":"Homebridge","Manufacturer":"Nest","Service":"Smoke Sensor","device":"HomebridgeCC:22:3D:E3:CE:30NestToy Room Smoke00000087","conf":"df59aa47.b26148","x":700,"y":3680,"wires":[["a71dfd23.87f56"]]},{"id":"6a2276e1.11abe8","type":"hb-event","z":"42fe707d.eeeb6","name":"Toy Room Hallway Smoke","Homebridge":"Homebridge","Manufacturer":"Nest","Service":"Smoke Sensor","device":"HomebridgeCC:22:3D:E3:CE:30NestToy Room Hallway Smoke00000087","conf":"df59aa47.b26148","x":730,"y":3760,"wires":[["7da06e93.46a86"]]},{"id":"55780ea6.90bed","type":"hb-event","z":"42fe707d.eeeb6","name":"Downstairs Carbon Monoxide","Homebridge":"Homebridge","Manufacturer":"Nest","Service":"Carbon Monoxide Sensor","device":"HomebridgeCC:22:3D:E3:CE:30NestDownstairs Carbon Monoxide0000007F","conf":"df59aa47.b26148","x":720,"y":1480,"wires":[["f74bc10.c45164"]]},{"id":"872c456a.322608","type":"hb-event","z":"42fe707d.eeeb6","name":"Eva Hallway Carbon Monoxide","Homebridge":"Homebridge","Manufacturer":"Nest","Service":"Carbon Monoxide Sensor","device":"HomebridgeCC:22:3D:E3:CE:30NestEva Hallway Carbon Monoxide0000007F","conf":"df59aa47.b26148","x":730,"y":1560,"wires":[["be6607cc.a7e608"]]},{"id":"df6a9c7b.4afc3","type":"hb-event","z":"42fe707d.eeeb6","name":"Evas Bedroom Carbon Monoxide","Homebridge":"Homebridge","Manufacturer":"Nest","Service":"Carbon Monoxide Sensor","device":"HomebridgeCC:22:3D:E3:CE:30NestEvas Bedroom Carbon Monoxide0000007F","conf":"df59aa47.b26148","x":730,"y":1640,"wires":[["5919086e.1267b8"]]},{"id":"ffff1ac2.1f66b8","type":"hb-event","z":"42fe707d.eeeb6","name":"Game Room Carbon Monoxide","Homebridge":"Homebridge","Manufacturer":"Nest","Service":"Carbon Monoxide Sensor","device":"HomebridgeCC:22:3D:E3:CE:30NestGame Room Carbon Monoxide0000007F","conf":"df59aa47.b26148","x":730,"y":1720,"wires":[["7c610971.6da498"]]},{"id":"887ff667.8273c8","type":"hb-event","z":"42fe707d.eeeb6","name":"Guest Bedroom Carbon Monoxide","Homebridge":"Homebridge","Manufacturer":"Nest","Service":"Carbon Monoxide Sensor","device":"HomebridgeCC:22:3D:E3:CE:30NestGuest Bedroom Carbon Monoxide0000007F","conf":"df59aa47.b26148","x":740,"y":1800,"wires":[["4bea9774.3c4798"]]},{"id":"2849c967.1aa2b6","type":"hb-event","z":"42fe707d.eeeb6","name":"Master Bedroom Carbon Monoxide","Homebridge":"Homebridge","Manufacturer":"Nest","Service":"Carbon Monoxide Sensor","device":"HomebridgeCC:22:3D:E3:CE:30NestMaster Bedroom Carbon Monoxide0000007F","conf":"df59aa47.b26148","x":740,"y":1880,"wires":[["a3738611.ca6328"]]},{"id":"b3d77415.6e4a98","type":"hb-event","z":"42fe707d.eeeb6","name":"Toy Room Carbon Monoxide","Homebridge":"Homebridge","Manufacturer":"N